{
  "name": "on-your-time-server",
  "version": "1.0.0",
  "description": "Server for the On Your Time subway schedule application",
  "scripts": {
    "test": "jasmine",
    "coverage": "node ./node_modules/.bin/istanbul cover jasmine",
    "check-coverage": "node ./node_modules/.bin/istanbul check-coverage || true",
    "debug": "node --inspect --debug-brk ./bin/www",
    "start": "node ./bin/www",
    "dev": "DEBUG=on-your-time:* npm start",
    "debug-test": "node --inspect --debug-brk node_modules/jasmine/bin/jasmine.js || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/klandell/on-your-time-server.git"
  },
  "author": "Kyle Landell <kyle.landell@gmail.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/klandell/on-your-time-server/issues"
  },
  "homepage": "https://github.com/klandell/on-your-time-server#readme",
  "dependencies": {
    "body-parser": "^1.16.1",
    "debug": "^2.6.1",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
    "morgan": "^1.8.1"
  },
  "devDependencies": {
    "istanbul": "^0.4.5",
    "jasmine": "^2.5.3",
    "redis": "^2.6.5",
    "rewire": "^2.5.2"
  }
}
