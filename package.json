  "bugs": {
    "url": "https://github.com/klandell/on-your-time-server/issues"
  },
  "homepage": "https://github.com/klandell/on-your-time-server#readme",
  "dependencies": {
    "body-parser": "^1.16.1",
    "debug": "^2.6.1",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
    "gtfs-mongoose": "0.0.8",
    "mongoose": "^4.8.4",
    "morgan": "^1.8.1"
},
  "engines": {
    "node": "~7.8.0"
  }
}
