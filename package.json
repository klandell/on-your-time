{
  "name": "on-your-time-server",
  "version": "1.0.0",
  "description": "Server for the On Your Time subway schedule application",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "debug": "node --inspect --debug-brk ./bin/www",
    "start": "node ./bin/www",
    "build:client": "NODE_ENV=production webpack --config ./client/webpack.config.js",
    "deploy": "npm run build:client && gcloud app deploy --verbosity=info && gcloud app browse"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/klandell/on-your-time-server.git"
